/* file -  steli - elide characters in string */
#include "c:\m\rid\rider.h"
#include "c:\m\rid\stdef.h"
/* code -  st_eli - string elide */
int st_eli(
char *mat ,
register char *src ,
register char *dst )
{ int cnt = 0;
  while ( *src) {
    if ( !st_mem (*src, mat)) {
      *dst++ = *src; }
    src++;
  } 
  *dst = 0;
  return ( cnt);
} 
