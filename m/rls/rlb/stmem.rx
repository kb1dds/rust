#include "c:\m\rid\rider.h"
#include "c:\m\rid\stdef.h"
/* code -  st_mem - string membership test */
int st_mem(
int cha ,
register char *str )
{ register char tmp ;
  if( str == NULL)return 0;
  while ( (tmp = *str++) != 0) {
    if( (char )tmp == (char )cha)return 1;
  } 
  return 0;
} 
